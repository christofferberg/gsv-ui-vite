{
  "name": "@adapt-agency/gsv-ui-vite",
  "author": "Adapt Agency",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "version": "0.0.5",
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "vite build",
    "vite:serve": "vite preview",
    "dev": "npm run vite:dev",
    "clean": "rimraf dist",
    "tsup": "tsup src/main.ts --format esm,cjs --dts --minify --sourcemap",
    "build": "run-s clean tsup",
    "serve": "npm run vite:serve",
    "release": "np"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@stitches/react": "^0.1.9",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "np": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "tslib": "^2.2.0",
    "tsup": "^4.11.0",
    "typescript": "^4.2.4",
    "vite": "^2.3.3",
    "vite-tsconfig-paths": "^3.3.10"
  }
