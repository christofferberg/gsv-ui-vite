{
  "name": "@adapt-agency/gsv-ui-vite",
  "author": "Adapt Agency",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "version": "0.0.3",
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "vite build",
    "vite:serve": "vite preview",
    "dev": "npm run vite:dev",
    "clean": "rimraf dist",
    "compile": "tsc --project tsconfig.build.json",
    "build": "run-s clean vite:build compile",
    "serve": "npm run vite:serve",
    "release": "np"
  },
  "dependencies": {
    "@stitches/react": "^0.1.9",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "np": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2",
    "vite": "^2.3.3",
    "vite-tsconfig-paths": "^3.3.10"
  }
}
